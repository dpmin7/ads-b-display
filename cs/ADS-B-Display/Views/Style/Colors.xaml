<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
    xmlns:system="clr-namespace:System;assembly=mscorlib"
    mc:Ignorable="po">
    <!--  Background  -->

    <SolidColorBrush x:Key="langTokens" />

    <SolidColorBrush
        x:Key="MahApps.Brushes.Background"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.SystemChromeLow}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.LightBackground"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.ThemeBackground}" />

    <!--  Foreground  -->
    <SolidColorBrush
        x:Key="MahApps.Brushes.Foreground"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.ThemeForeground}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.WindowTitle.Foreground"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.ThemeForeground}" />

    <!--  General  -->
    <SolidColorBrush
        x:Key="MahApps.Brushes.BackgroundHighlighted"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.Gray8}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Border.Highlighted"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.Gray}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.BackgroundSelected"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.Accent}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.ForegroundSelected"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.IdealForeground}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Border.Selected"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.Highlight}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Background.Normal"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.Gray9}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Border.Normal"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.Gray6}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Window.Glow"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.Accent}" />

    <!--  Text Box  -->
    <SolidColorBrush
        x:Key="MahApps.Brushes.TextBox.Background"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.ThemeBackground}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.TextBox.BackgroundSelected"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.ThemeBackground}" />

    <!--  Search Text Box  -->
    <SolidColorBrush
        x:Key="MahApps.Brushes.SearchTextBox.Foreground"
        po:Freeze="True"
        Opacity="0.3"
        Color="{StaticResource MahApps.Colors.ThemeForeground}" />

    <!--  Link Button  -->
    <SolidColorBrush
        x:Key="MahApps.Brushes.LinkButton.Foreground"
        po:Freeze="True"
        Color="#1297FB" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.LinkButton.ForegroundHighlighted"
        po:Freeze="True"
        Color="#55AAFF" />

    <!--  Close Button  -->
    <SolidColorBrush
        x:Key="MahApps.Brushes.CloseButton.BackgroundHighlighted"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.Accent}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.CloseButton.BackgroundPressed"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.AccentBase}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.CloseButton.Stroke"
        po:Freeze="True"
        Opacity="0.5"
        Color="{StaticResource MahApps.Colors.ThemeForeground}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.CloseButton.StrokeHighlighted"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.ThemeForeground}" />

    <!--  Menu  -->
    <SolidColorBrush
        x:Key="MahApps.Brushes.Menu.Checkmark.Fill"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.Accent}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Menu.Checkmark.Background"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.Highlight}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.MenuSeparator.Border"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.Gray.SemiTransparent}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.MenuItem.BackgroundHighlighted"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.Gray8}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.SubmenuItem.Background"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.ThemeBackground}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.SubmenuItem.BackgroundHighlighted"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.Gray8}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Menu.ForegroundDisabled"
        po:Freeze="True"
        Color="#FF7F7F7F" />
    <SolidColorBrush
        x:Key="{x:Static SystemColors.MenuTextBrushKey}"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.ThemeForeground}" />

    <!--  Scroll Bar  -->
    <SolidColorBrush
        x:Key="MahApps.Brushes.ScrollBar.PageButtonBackgroundHighlighted"
        po:Freeze="True"
        Color="{StaticResource MahApps.Colors.Gray.SemiTransparent}" />

    <!--  Notification Button  -->
    <SolidColorBrush x:Key="MahApps.Brushes.NotificationButton.Foreground" Color="#000000" />
    <SolidColorBrush x:Key="MahApps.Brushes.NotificationButton.Background" Color="#FFCC00" />
    <SolidColorBrush x:Key="MahApps.Brushes.NotificationButton.BackgroundOver" Color="#FFDF66" />
    <SolidColorBrush x:Key="MahApps.Brushes.NotificationButton.BackgroundPressed" Color="#C59E00" />

    <!--  Quick Launch TextBox  -->
    <SolidColorBrush x:Key="MahApps.Brushes.QuickLaunchTextBox.Border" Color="{StaticResource MahApps.Colors.Gray9}" />
    <SolidColorBrush x:Key="MahApps.Brushes.QuickLaunchTextBox.FocusedBorder" Color="{StaticResource MahApps.Colors.Accent}" />

    <SolidColorBrush
        x:Key="BLGRed"
        po:Freeze="True"
        Color="#A50034" />

    <!--  Device Status  -->
    <SolidColorBrush x:Key="MahApps.Brushes.DeviceStatus.Active" Color="#0BD02E" />
    <SolidColorBrush x:Key="MahApps.Brushes.DeviceStatus.Deactive" Color="#787272" />

    <SolidColorBrush x:Key="Aurora.Brushes.Title.Background" Color="{StaticResource Aurora.Colors.Title.Background}" />

    <SolidColorBrush x:Key="Aurora.Brushes.Title.Component.Background" Color="{StaticResource Aurora.Colors.Title.Component.Background}" />

    <SolidColorBrush x:Key="Aurora.Brushes.GroupBox.GroupBox20" Color="{StaticResource Aurora.Colors.GroupBox20}" />

    <SolidColorBrush x:Key="Aurora.Brushes.GroupBox.GroupBox21" Color="{StaticResource Aurora.Colors.GroupBox21}" />

    <SolidColorBrush x:Key="Aurora.Brushes.GroupBox.GroupBox22" Color="{StaticResource Aurora.Colors.GroupBox22}" />


    <SolidColorBrush x:Key="Aurora.Brushes.GroupBox.GroupBox23" Color="{StaticResource Aurora.Colors.GroupBox23}" />

    <SolidColorBrush x:Key="Aurora.Brushes.GroupBox.GroupBox24" Color="{StaticResource Aurora.Colors.GroupBox24}" />

    <SolidColorBrush x:Key="Aurora.Brushes.GroupBox.Shadow" Color="{StaticResource Aurora.Colors.GroupBox.Shadow}" />

    <SolidColorBrush x:Key="Aurora.Brushes.Header.Background" Color="{StaticResource Aurora.Colors.Header.Background}" />

    <SolidColorBrush x:Key="Aurora.Brushes.Header.Border" Color="{StaticResource Aurora.Colors.Header.Border}" />

</ResourceDictionary>