<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:PresentationOptions="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--  Aurora2.0 Style  -->
    <Color x:Key="Aurora.Colors.Title.Background">Black</Color>
    <Color x:Key="Aurora.Colors.Title.Component.Background">Black</Color>

    <Color x:Key="Aurora.Colors.GroupBox20">#70282828</Color>
    <Color x:Key="Aurora.Colors.GroupBox21">#262f3e</Color>
    <Color x:Key="Aurora.Colors.GroupBox22">#1c1c1d</Color>
    <Color x:Key="Aurora.Colors.GroupBox23">#272b31</Color>
    <Color x:Key="Aurora.Colors.GroupBox24">#3b4a5a</Color>
    <Color x:Key="Aurora.Colors.GroupBox.Shadow">#000000</Color>

    <Color x:Key="Aurora.Colors.Header.Background">#262f3d</Color>
    <Color x:Key="Aurora.Colors.Header.Border">Black</Color>

    <!--  MahApps Style  -->
    <Color x:Key="MahApps.Colors.Accent3">#660078D7</Color>

    <Color x:Key="MahApps.Colors.ThemeForeground">#FFFFFFFF</Color>
    <Color x:Key="MahApps.Colors.ThemeBackground">#FF2E2E2E</Color>
    <Color x:Key="MahApps.Colors.IdealForeground">White</Color>
    <Color x:Key="MahApps.Colors.Gray3">#FF9D9D9D</Color>
    <Color x:Key="MahApps.Colors.Gray8">#FF454545</Color>
    <Color x:Key="MahApps.Colors.Gray9">#FF333337</Color>
    <Color x:Key="MahApps.Colors.Accent">#A50034</Color>
    <Color x:Key="MahApps.Colors.Accent2">#990078D7</Color>
    <Color x:Key="Aurora.Colors.BMSDarkGray">#3E3E42</Color>

    <CornerRadius x:Key="Aurora.DefaultCornerRadius">0</CornerRadius>
    <Thickness x:Key="Aurora.Button.Border.Selected.Thickness">1</Thickness>
    <system:Double x:Key="Aurora.Body.Header.Height">42</system:Double>
    <system:Double x:Key="Aurora.Body.Header.Icon.Size">36</system:Double>
    <system:Double x:Key="Aurora.TextBox.FontSize">14</system:Double>

    <SolidColorBrush x:Key="Aurora.Brushes.Accent" Color="{DynamicResource MahApps.Colors.Accent}" />
    <SolidColorBrush x:Key="Aurora.Brushes.Accent2" Color="{DynamicResource MahApps.Colors.Accent2}" />
    <SolidColorBrush x:Key="KeyTextColor" Color="#ffffff" />
    <!--  Foreground  -->
    <SolidColorBrush x:Key="Aurora.Brushes.GroupBox.GroupBox20" Color="{DynamicResource Aurora.Colors.GroupBox20}" />

    <SolidColorBrush x:Key="Aurora.Brushes.GroupBox.GroupBox21" Color="{DynamicResource Aurora.Colors.GroupBox21}" />

    <SolidColorBrush x:Key="Aurora.Brushes.GroupBox.GroupBox22" Color="{DynamicResource Aurora.Colors.GroupBox22}" />


    <SolidColorBrush x:Key="Aurora.Brushes.GroupBox.GroupBox23" Color="{DynamicResource Aurora.Colors.GroupBox23}" />

    <SolidColorBrush x:Key="Aurora.Brushes.GroupBox.GroupBox24" Color="{DynamicResource Aurora.Colors.GroupBox24}" />

    <SolidColorBrush x:Key="Aurora.Brushes.GroupBox.Shadow" Color="{DynamicResource Aurora.Colors.GroupBox.Shadow}" />

    <SolidColorBrush x:Key="Aurora.Brushes.Header.Background" Color="{DynamicResource Aurora.Colors.Header.Background}" />

    <SolidColorBrush x:Key="Aurora.Brushes.Header.Border" Color="{DynamicResource Aurora.Colors.Header.Border}" />

    <SolidColorBrush x:Key="Aurora.Brushes.Title.Background" Color="{StaticResource Aurora.Colors.Title.Background}" />

    <SolidColorBrush x:Key="Aurora.Brushes.Title.Component.Background" Color="{StaticResource Aurora.Colors.Title.Component.Background}" />

    <SolidColorBrush x:Key="KeyboardBackgroundBrush" Color="#2f2f2f" />
    <SolidColorBrush x:Key="KeyboardBackgroundAccentBrush" Color="#FFA0A0A0" />
    <SolidColorBrush x:Key="KeyboardForegroundBrush" Color="White" />

    <SolidColorBrush
        x:Key="MahApps.Brushes.Foreground"
        PresentationOptions:Freeze="True"
        Color="{DynamicResource MahApps.Colors.ThemeForeground}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Window.Background"
        PresentationOptions:Freeze="True"
        Color="#121212" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Border.NonActive"
        PresentationOptions:Freeze="True"
        Color="#808080" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.ThemeBackground"
        PresentationOptions:Freeze="True"
        Color="{StaticResource MahApps.Colors.ThemeBackground}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.ThemeForeground"
        PresentationOptions:Freeze="True"
        Color="{StaticResource MahApps.Colors.ThemeForeground}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Background.Normal"
        PresentationOptions:Freeze="True"
        Color="{StaticResource MahApps.Colors.Gray9}" />
    <SolidColorBrush x:Key="MahApps.Brushes.Border" Color="#9E9E9E" />
    <SolidColorBrush x:Key="MahApps.Brushes.Button.Border" Color="#9E9E9E" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Gray3"
        PresentationOptions:Freeze="True"
        Color="{StaticResource MahApps.Colors.Gray3}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.BackgroundSelected"
        PresentationOptions:Freeze="True"
        Color="{StaticResource MahApps.Colors.Accent}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.ForegroundSelected"
        PresentationOptions:Freeze="True"
        Color="{StaticResource MahApps.Colors.IdealForeground}" />

    <SolidColorBrush
        x:Key="MahApps.Brushes.BackgroundHighlighted"
        PresentationOptions:Freeze="True"
        Color="{StaticResource MahApps.Colors.Gray8}" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Selected.Foreground"
        PresentationOptions:Freeze="True"
        Color="{StaticResource MahApps.Colors.IdealForeground}" />
    <!--  Machine Style  -->
    <SolidColorBrush
        x:Key="Machine.Brushes.Button.Background"
        PresentationOptions:Freeze="True"
        Color="{StaticResource Aurora.Colors.BMSDarkGray}" />

    <SolidColorBrush
        x:Key="Aurora.Brushes.TabItem.Background.Normal"
        PresentationOptions:Freeze="True"
        Color="#3e3e3e" />
    <SolidColorBrush
        x:Key="Aurora.Brushes.TabItem.Foreground.Selected"
        PresentationOptions:Freeze="True"
        Color="White" />
    <SolidColorBrush
        x:Key="Aurora.Brushes.TabItem.Foreground"
        PresentationOptions:Freeze="True"
        Color="#bbbbbb" />
    <SolidColorBrush
        x:Key="Aurora.Brushes.TabItem.Background"
        PresentationOptions:Freeze="True"
        Color="#1e1e1e" />
    <SolidColorBrush
        x:Key="Aurora.Brushes.TabControl.Background"
        PresentationOptions:Freeze="True"
        Color="#1e1e1e" />
    <SolidColorBrush
        x:Key="Aurora.Brushes.TabControl.BorderBrush"
        PresentationOptions:Freeze="True"
        Color="#333333" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Aurora.Datagrid.Background"
        PresentationOptions:Freeze="True"
        Color="#1e1e1e" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Aurora.Datagrid.Border"
        PresentationOptions:Freeze="True"
        Color="#999999" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Aurora.Datagrid.RowHeader.Background"
        PresentationOptions:Freeze="True"
        Color="#3e3e3e" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Aurora.Datagrid.RowHeader.Foreground"
        PresentationOptions:Freeze="True"
        Color="#bbbbbb" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Aurora.Datagrid.ColumnHeader.Background"
        PresentationOptions:Freeze="True"
        Color="#3e3e3e" />
    <SolidColorBrush
        x:Key="MahApps.Brushes.Aurora.Datagrid.ColumnHeader.Foreground"
        PresentationOptions:Freeze="True"
        Color="#bbbbbb" />
    <SolidColorBrush
        x:Key="Aurora.Datagrid.MouseOver"
        PresentationOptions:Freeze="True"
        Color="#f6e8ca" />

    <SolidColorBrush x:Key="ACGrayDark" Color="#141414" />
    <SolidColorBrush x:Key="ACGrayDeep" Color="#4A4A4A" />
    <SolidColorBrush x:Key="ACGrayNormal" Color="#2A2A2A" />
    <SolidColorBrush x:Key="ACGraySemiLight" Color="#991A1A1A" />
    <SolidColorBrush x:Key="ACGrayLight" Color="#993A3A3A" />
    <SolidColorBrush x:Key="ACGrayBorder" Color="#5A5A5A" />

    <!--  KeyTextControl Default  -->
    <SolidColorBrush x:Key="DefaultKeyTextColor" Color="{StaticResource MahApps.Colors.ThemeForeground}" />
    <SolidColorBrush x:Key="DefaultValueTextColor" Color="{StaticResource MahApps.Colors.ThemeForeground}" />
    <SolidColorBrush x:Key="DefaultValueTextBgColor" Color="Transparent" />

    <DrawingImage x:Key="Electronics_MouseDrawingImage">
        <DrawingImage.Drawing>
            <DrawingGroup ClipGeometry="M0,0 V32 H32 V0 H0 Z">
                <DrawingGroup Opacity="1">
                    <GeometryDrawing Brush="#A0A2A2A2" Geometry="F1 M32,32z M0,0z M16,4.1L16,12 23.9,12C23.5,7.8,20.2,4.5,16,4.1z" />
                    <GeometryDrawing Brush="#A0A2A2A2" Geometry="F1 M32,32z M0,0z M14,4.1C9.8,4.5,6.5,7.8,6.1,12L14,12 14,4.1z" />
                    <GeometryDrawing Brush="#A0A2A2A2" Geometry="F1 M32,32z M0,0z M16,14L14,14 6,14 6,19C6,24 10.1,28 15,28 19.9,28 24,24 24,19L24,14 16,14z" />
                </DrawingGroup>
            </DrawingGroup>
        </DrawingImage.Drawing>
    </DrawingImage>
</ResourceDictionary>